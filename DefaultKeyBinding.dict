// Cmd+Ctrl+Up: Move line up
"@^\UF700" = (
    "moveToEndOfLine:",
    "deleteToBeginningOfLine:",
    "deleteBackward:",
    "moveToBeginningOfLine:",
    "insertNewline:",
    "moveUp:",
