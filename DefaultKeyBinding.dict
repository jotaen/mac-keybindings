// Cmd+Ctrl+Down: Move line down
"@^\UF701" = (
    "moveToEndOfLine:",
    "deleteToBeginningOfLine:",
    "deleteForward:",
    "moveToEndOfLine:",
    "insertNewline:",
    "yank:" );

// Delete from current cursor position to end of line
"@\UF728" = (
    "deleteToEndOfLine:" );
}
